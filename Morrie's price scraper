import requests
import csv
from bs4 import BeautifulSoup
page = requests.get('http://www.goldenvalley-cadillac.com/VehicleSearchResults?search=new')
soup = BeautifulSoup(page.text, 'html.parser')


prices = soup.findAll('span', attrs={'itemprop': 'price'})
for price in prices:
    print(price.getText())


with open("page1.csv", "a") as csv_file:
	writer = csv.writer(csv_file)
	writer.writerow([prices])
